let WHITE = object
begin
    let black = false;
    function flip() -> BLACK;
    function is_black() -> false;
    function is_white() -> true;
    function draw() -> print(".");
end;

let BLACK = object
begin
    let black = true;
    function flip() -> WHITE;
    function is_black() -> true;
    function is_white() -> false;
    function draw() -> print("#");
end;

